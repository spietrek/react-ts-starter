{
  "name": "react-starter",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "spietrek"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "watch:tailwind": "npx tailwindcss -i ./src/styles/index.css -o ./app/styles/index.css --watch",
    "type:check": "tsc",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore src",
    "lintfix": "eslint --ext .ts,.tsx --ignore-path .gitignore --fix src",
    "build:css": "postcss src/styles/index.css -o app/styles/index.css --env production",
    "preview:css": "postcss src/styles/index.css -o app/assets/index.css",
    "build:run": "yarn build:css && yarn build",
    "data-server": "json-server --watch app/db.json --port 3004"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@reduxjs/toolkit": "^1.8.1",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "daisyui": "^2.13.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@storybook/addon-a11y": "^6.4.21",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/node-logger": "^6.4.21",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.21",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^14.1.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "@types/zxcvbn": "^4.4.1",
    "autoprefixer": "^10.4.4",
    "craco-alias": "^3.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-security": "^1.4.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-nested-ancestors": "^2.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "react-scripts": "5.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14"
  }
}
